{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "A template to add ACL Entries.",
  "Outputs" : {
    "VPCName" : {
      "Description": "The name of the VPC",
      "Value" : { "Ref" : "myVpcName" }
      }
  },
  "Parameters" : {
    "myVpcName" : {
      "Description" : "The VPC to use",
      "Type" : "String"
    },
    "myNetworkAcl" : {
      "Description" : "The acl to use",
      "Type" : "String"
    }
  },
  "Resources": {
    "acl3": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Egress": "true",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        }
      }
    },
    "acl4": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        }
      }
    }
  }
}
