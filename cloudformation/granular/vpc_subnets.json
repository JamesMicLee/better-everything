{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "A template to add subnets.",
  "Outputs": {
    "Subnet1Lambda": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetlaa"
      }
    },
    "Subnet2Lambda": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetlab"
      }
    },
    "Subnet3Lambda": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetlac"
      }
    },
    "Subnet4Ec2": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetec2a"
      }
    },
    "Subnet5Ec2": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetec2b"
      }
    },
    "Subnet6Ec2": {
      "Description": "The name of a subnet",
      "Value": {
        "Ref": "mySubnetec2c"
      }
    },
    "VPCName": {
      "Description": "The name of the VPC",
      "Value": {
        "Ref": "myVpcName"
      }
    }
  },
  "Parameters": {
    "myNetworkAcl": {
      "Description": "The ACL to use",
      "Type": "String"
    },
    "myVpcName": {
      "Description": "The VPC to use",
      "Type": "String"
    }
  },
  "Resources": {
    "mySubnetacl10": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetec2b"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetacl11": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetlac"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetacl12": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetec2a"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetacl7": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetec2c"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetacl8": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetlab"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetacl9": {
      "Properties": {
        "NetworkAclId": {
          "Ref": "myNetworkAcl"
        },
        "SubnetId": {
          "Ref": "mySubnetlaa"
        }
      },
      "Type": "AWS::EC2::SubnetNetworkAclAssociation"
    },
    "mySubnetec2a": {
      "Properties": {
        "AvailabilityZone": "eu-west-2a",
        "CidrBlock": "10.0.0.0/28",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    },
    "mySubnetec2b": {
      "Properties": {
        "AvailabilityZone": "eu-west-2b",
        "CidrBlock": "10.0.0.16/28",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    },
    "mySubnetec2c": {
      "Properties": {
        "AvailabilityZone": "eu-west-2c",
        "CidrBlock": "10.0.0.32/28",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    },
    "mySubnetlaa": {
      "Properties": {
        "AvailabilityZone": "eu-west-2a",
        "CidrBlock": "10.0.4.0/22",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    },
    "mySubnetlab": {
      "Properties": {
        "AvailabilityZone": "eu-west-2b",
        "CidrBlock": "10.0.8.0/22",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    },
    "mySubnetlac": {
      "Properties": {
        "AvailabilityZone": "eu-west-2c",
        "CidrBlock": "10.0.12.0/22",
        "Tags": [
          {
            "Key": "Name",
            "Value": "myVPCTag"
          }
        ],
        "VpcId": {
          "Ref": "myVpcName"
        }
      },
      "Type": "AWS::EC2::Subnet"
    }
  }
}
